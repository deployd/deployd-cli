{
  "name": "deployd-cli",
  "version": "2.0.0",
  "license": "MIT",
  "description": "The deployd command line interface",
  "repository": {
    "url": "git://github.com/deployd/deployd-cli.git"
  },
  "scripts": {
    "precommit": "lint-staged",
    "test": "npm run lint",
    "lint": "eslint . --config .eslintrc.js --quiet --fix"
  },
